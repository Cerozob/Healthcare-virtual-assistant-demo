# Healthcare Agent Configuration
# Copy this file to .env and fill in your actual values

# Bedrock Model Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0
MODEL_TEMPERATURE=0.1
MODEL_MAX_TOKENS=4096
MODEL_TOP_P=0.9

# Bedrock Knowledge Base
BEDROCK_KNOWLEDGE_BASE_ID=your-knowledge-base-id-here

# Bedrock Guardrails (optional)
BEDROCK_GUARDRAIL_ID=your-guardrail-id-here
BEDROCK_GUARDRAIL_VERSION=DRAFT

# AgentCore Gateway Configuration
MCP_GATEWAY_URL=https://agentcoregateway-xxxxxxxxxx.gateway.bedrock-agentcore.us-east-1.amazonaws.com
GATEWAY_ID=your-gateway-id-here

# AWS Configuration
AWS_REGION=us-east-1

# Session Storage
SESSION_BUCKET=your-session-bucket-name

# Agent Configuration
DEFAULT_LANGUAGE=es-LATAM

# Observability
ENABLE_TRACING=true
LOG_LEVEL=INFO
