{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://healthcare-agent.com/schemas/agentcore-response.json",
  "title": "AgentCore Healthcare Agent Response",
  "description": "Response schema for healthcare agent running on AWS Bedrock AgentCore",
  "type": "object",
  "required": ["response", "sessionId", "status"],
  "properties": {
    "response": {
      "type": "string",
      "description": "The agent's response content/message",
      "minLength": 1,
      "maxLength": 100000
    },
    "sessionId": {
      "type": "string",
      "description": "Session identifier for conversation continuity",
      "minLength": 33,
      "maxLength": 200,
      "pattern": "^[a-zA-Z0-9_-]+$"
    },
    "patientContext": {
      "type": ["object", "null"],
      "description": "Patient context information extracted by the agent",
      "properties": {
        "patientId": {
          "type": "string",
          "description": "Patient identifier (cedula, medical record number, or generated ID)",
          "examples": ["12345678", "juan_perez", "patient_001"]
        },
        "patientName": {
          "type": "string",
          "description": "Patient full name",
          "examples": ["Juan Pérez", "María García López", "Paciente -"]
        },
        "contextChanged": {
          "type": "boolean",
          "description": "Indicates if patient context changed in this interaction"
        },
        "identificationSource": {
          "type": "string",
          "enum": [
            "tool_extraction",
            "agent_extraction",
            "content_extraction", 
            "image_analysis",
            "document_analysis",
            "multimodal_analysis",
            "default"
          ],
          "description": "How the patient was identified by the agent"
        },
        "fileOrganizationId": {
          "type": "string",
          "description": "AI-determined ID for file organization in S3"
        },
        "aiIdentificationData": {
          "type": "object",
          "description": "Additional AI identification metadata",
          "properties": {
            "primary_identifier_type": {
              "type": "string",
              "examples": ["cedula", "name", "medical_record"]
            },
            "identification_confidence": {
              "type": "string",
              "examples": ["high", "medium", "low"]
            }
          },
          "additionalProperties": true
        }
      },
      "additionalProperties": false
    },
    "memoryEnabled": {
      "type": "boolean",
      "description": "Indicates if AgentCore Memory is enabled for this session"
    },
    "memoryId": {
      "type": ["string", "null"],
      "description": "AgentCore Memory ID if memory is enabled",
      "examples": ["healthcare-memory-prod", "healthcare-memory-dev", null]
    },
    "uploadResults": {
      "type": "array",
      "description": "Results of multimodal content uploads to S3",
      "items": {
        "type": "object",
        "required": ["success"],
        "properties": {
          "success": {
            "type": "boolean",
            "description": "Whether the upload was successful"
          },
          "content_type": {
            "type": "string",
            "description": "Type of content uploaded",
            "examples": ["image", "document", "file"]
          },
          "s3_url": {
            "type": "string",
            "description": "S3 URL of the uploaded content",
            "format": "uri"
          },
          "error": {
            "type": "string",
            "description": "Error message if upload failed"
          },
          "patient_id": {
            "type": "string",
            "description": "Patient ID used for organizing the upload"
          },
          "file_name": {
            "type": "string",
            "description": "Original filename"
          },
          "file_size": {
            "type": "number",
            "description": "File size in bytes"
          }
        },
        "additionalProperties": true
      }
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "ISO 8601 timestamp of response generation"
    },
    "status": {
      "type": "string",
      "enum": ["success", "error"],
      "description": "Response status"
    },
    "metrics": {
      "type": "object",
      "description": "Strands execution metrics and metadata",
      "properties": {
        "stopReason": {
          "type": "string",
          "description": "Reason why the agent stopped generating content"
        },
        "metricsSummary": {
          "type": "object",
          "description": "Summary of execution metrics from Strands EventLoopMetrics",
          "additionalProperties": true
        },
        "interrupts": {
          "type": "array",
          "description": "List of interrupts that occurred during execution",
          "items": {
            "type": "object",
            "additionalProperties": true
          }
        },
        "structuredOutputUsed": {
          "type": "boolean",
          "description": "Whether structured output was successfully used"
        }
      },
      "additionalProperties": false
    },
    "guardrailInterventions": {
      "type": "array",
      "description": "Guardrail interventions detected during processing (shadow mode monitoring)",
      "items": {
        "type": "object",
        "required": ["source", "action"],
        "properties": {
          "source": {
            "type": "string",
            "enum": ["INPUT", "OUTPUT"],
            "description": "Whether the intervention was on user input or agent output"
          },
          "action": {
            "type": "string",
            "enum": ["GUARDRAIL_INTERVENED", "NONE"],
            "description": "Action taken by guardrail (in shadow mode, content is never blocked)"
          },
          "content_preview": {
            "type": "string",
            "description": "First 100 characters of flagged content",
            "maxLength": 100
          },
          "timestamp": {
            "type": "string",
            "description": "When the evaluation occurred"
          },
          "violations": {
            "type": "array",
            "description": "Specific violations detected by guardrail policies",
            "items": {
              "type": "object",
              "required": ["type"],
              "properties": {
                "type": {
                  "type": "string",
                  "enum": ["topic_policy", "content_policy", "pii_entity", "pii_regex", "grounding_policy"],
                  "description": "Type of violation detected"
                },
                "topic": {
                  "type": "string",
                  "description": "Topic name for topic_policy violations",
                  "examples": ["TradingCriptomonedas", "DeportesExtremosPeligrosos"]
                },
                "content_type": {
                  "type": "string",
                  "description": "Content type for content_policy violations",
                  "enum": ["HATE", "INSULTS", "VIOLENCE", "SEXUAL"],
                  "examples": ["VIOLENCE", "HATE"]
                },
                "pii_type": {
                  "type": "string",
                  "description": "PII type for pii_entity violations",
                  "examples": ["EMAIL", "PHONE", "ADDRESS", "CREDIT_DEBIT_CARD_NUMBER"]
                },
                "pattern": {
                  "type": "string",
                  "description": "Pattern name for pii_regex violations",
                  "examples": ["CedulaColombia", "HistoriaClinica", "TelefonoLatam"]
                },
                "grounding_type": {
                  "type": "string",
                  "description": "Grounding type for grounding_policy violations",
                  "enum": ["GROUNDING", "RELEVANCE"]
                },
                "confidence": {
                  "type": "string",
                  "description": "Confidence level for content_policy violations",
                  "enum": ["LOW", "MEDIUM", "HIGH"]
                },
                "score": {
                  "type": "number",
                  "description": "Score for grounding_policy violations",
                  "minimum": 0,
                  "maximum": 1
                },
                "threshold": {
                  "type": "number",
                  "description": "Threshold for grounding_policy violations",
                  "minimum": 0,
                  "maximum": 1
                },
                "action": {
                  "type": "string",
                  "description": "Action for this specific violation",
                  "enum": ["BLOCK", "ANONYMIZE", "NONE"],
                  "examples": ["BLOCK", "ANONYMIZE"]
                },
                "policy_type": {
                  "type": "string",
                  "description": "Policy type for topic_policy violations",
                  "enum": ["DENY", "ALLOW"]
                }
              },
              "additionalProperties": false
            }
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false,
  "examples": [
    {
      "response": "He encontrado el historial médico de Juan Pérez. El paciente tiene 45 años y presenta las siguientes condiciones médicas activas...",
      "sessionId": "agentcore_session_12345678-1234-1234-1234-123456789012",
      "patientContext": {
        "patientId": "12345678",
        "patientName": "Juan Pérez",
        "contextChanged": true,
        "identificationSource": "tool_extraction"
      },
      "memoryEnabled": true,
      "memoryId": "healthcare-memory-prod",
      "uploadResults": [],
      "timestamp": "2024-11-05T10:30:00.000Z",
      "status": "success",
      "metrics": {
        "stopReason": "end_turn",
        "metricsSummary": {
          "tool_calls": 2,
          "total_time_ms": 1250
        },
        "interrupts": [],
        "structuredOutputUsed": true
      }
    },
    {
      "response": "He analizado el documento médico adjunto. Se trata de un informe de laboratorio para la paciente María García...",
      "sessionId": "healthcare_session_abcdef01-2345-6789-abcd-ef0123456789",
      "patientContext": {
        "patientId": "maria_garcia",
        "patientName": "María García López",
        "contextChanged": true,
        "identificationSource": "agent_extraction",
        "fileOrganizationId": "maria_garcia_lopez"
      },
      "memoryEnabled": true,
      "memoryId": "healthcare-memory-prod",
      "uploadResults": [
        {
          "success": true,
          "content_type": "document",
          "s3_url": "s3://healthcare-bucket/maria_garcia/documents/lab_report_2024.pdf",
          "patient_id": "maria_garcia",
          "file_name": "lab_report_2024.pdf",
          "file_size": 245760
        }
      ],
      "timestamp": "2024-11-05T10:35:00.000Z",
      "status": "success"
    },
    {
      "response": "Lo siento, no puedo proporcionar consejos sobre inversiones en criptomonedas. ¿Puedo ayudarte con alguna consulta médica?",
      "sessionId": "healthcare_session_xyz789-4567-8901-cdef-234567890123",
      "patientContext": null,
      "memoryEnabled": true,
      "memoryId": "healthcare-memory-prod",
      "uploadResults": [],
      "timestamp": "2024-11-05T10:40:00.000Z",
      "status": "success",
      "metrics": {
        "stopReason": "end_turn",
        "metricsSummary": {
          "tool_calls": 0,
          "total_time_ms": 850
        },
        "interrupts": [],
        "structuredOutputUsed": false
      },
      "guardrailInterventions": [
        {
          "source": "INPUT",
          "action": "GUARDRAIL_INTERVENED",
          "content_preview": "¿Cuál es la mejor criptomoneda para invertir ahora?",
          "timestamp": "2024-11-05T10:40:00.123Z",
          "violations": [
            {
              "type": "topic_policy",
              "topic": "TradingCriptomonedas",
              "action": "BLOCK",
              "policy_type": "DENY"
            }
          ]
        }
      ]
    },
    {
      "response": "He revisado la información del paciente. Los resultados muestran valores normales en todos los parámetros.",
      "sessionId": "healthcare_session_pqr456-7890-1234-5678-901234567890",
      "patientContext": {
        "patientId": "87654321",
        "patientName": "Paciente -",
        "contextChanged": true,
        "identificationSource": "content_extraction",
        "fileOrganizationId": "87654321"
      },
      "memoryEnabled": true,
      "memoryId": "healthcare-memory-prod",
      "uploadResults": [],
      "timestamp": "2024-11-05T10:45:00.000Z",
      "status": "success",
      "metrics": {
        "stopReason": "end_turn",
        "metricsSummary": {
          "tool_calls": 1,
          "total_time_ms": 1100
        },
        "interrupts": [],
        "structuredOutputUsed": true
      },
      "guardrailInterventions": [
        {
          "source": "INPUT",
          "action": "GUARDRAIL_INTERVENED",
          "content_preview": "El paciente con cédula 87654321 necesita revisar sus resultados de laboratorio",
          "timestamp": "2024-11-05T10:45:00.100Z",
          "violations": [
            {
              "type": "pii_regex",
              "pattern": "CedulaColombia",
              "action": "ANONYMIZE"
            }
          ]
        },
        {
          "source": "OUTPUT",
          "action": "GUARDRAIL_INTERVENED",
          "content_preview": "He revisado la información del paciente con cédula 87654321. Los resultados muestran valores normales...",
          "timestamp": "2024-11-05T10:45:01.200Z",
          "violations": [
            {
              "type": "pii_regex",
              "pattern": "CedulaColombia",
              "action": "ANONYMIZE"
            }
          ]
        }
      ]
    }
  ]
}
