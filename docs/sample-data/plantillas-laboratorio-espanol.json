    }
  ],
  "interpretacion_banderas": {
    "A": "Alto - Por encima del rango de referencia",
    "B": "Bajo - Por debajo del rango de referencia",
    "AA": "Muy Alto - Significativamente elevado",
    "BB": "Muy Bajo - Significativamente disminuido",
    "": "Normal - Dentro del rango de referencia"
  }
}
